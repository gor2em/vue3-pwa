{
  "registerType": "autoUpdate",
  "includeAssets": [
    "favicon-32x32.png",
    "apple-touch-icon.png",
    "masked-icon.svg"
  ],
  "manifest": {
    "name": "vuepwa",
    "short_name": "vuepwa",
    "description": "vuepwa app created with vite",
    "theme_color": "#000000",
